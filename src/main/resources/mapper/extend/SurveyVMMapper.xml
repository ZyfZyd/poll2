<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.apps.poll.dao.extend.SurveyVMMapper">
  <select id="selectAll" resultMap="SurveyVMResultMap">
  	select * from poll_survey
  	<!-- id status code surveyDate questionnaire_id course_id  user_id clazz_id-->
  </select>
  
  <resultMap type="com.briup.apps.poll.bean.extend.SurveyVM" id="SurveyVMResultMap">
  	<id column="id" property="id"/>
  	<result column="status" property="status"/>
  	<result column="code" property="code"/>
  	<result column="surveyDate" property="surveyDate"/>
  	<association  column="questionnaire_id" property="questionnaire" select="com.briup.apps.poll.dao.QuestionnaireMapper.selectByPrimaryKey" ></association>
  	<association  column="course_id" property="course" select="com.briup.apps.poll.dao.CourseMapper.selectByPrimaryKey" ></association>
  	<association  column="user_id" property="user" select="com.briup.apps.poll.dao.UserMapper.selectByPrimaryKey" ></association>
  	<association  column="clazz_id" property="clazz" select="com.briup.apps.poll.dao.ClazzMapper.selectByPrimaryKey" ></association>
  </resultMap>
</mapper>